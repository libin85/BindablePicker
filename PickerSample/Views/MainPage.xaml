<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:controls="clr-namespace:PickerSample.Controls;assembly=PickerSample"
    xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms" 
    prism:ViewModelLocator.AutowireViewModel="True" 
    x:Class="PickerSample.Views.MainPage" Title="MainPage">
	<StackLayout HorizontalOptions="Center" VerticalOptions="Center">
		<controls:LabelWithPickerControl WidthRequest="90" HorizontalOptions="End" ItemSource="{Binding BinCodes}"
            Margin="0,0,0,7" />
	</StackLayout>
</ContentPage>
